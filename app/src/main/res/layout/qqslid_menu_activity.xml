<?xml version="1.0" encoding="utf-8"?>
<com.dengzi.slidingmenu.QQSlidingMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_sliding_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/slid_menu_bg"
    android:orientation="vertical"
    app:menuRightMargin="20dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <FrameLayout
            android:id="@+id/menu_fl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></FrameLayout>

        <FrameLayout
            android:id="@+id/content_fl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></FrameLayout>
        <!--
                <include layout="@layout/slid_home_menu" />

                <include layout="@layout/slid_home_content" />-->
    </LinearLayout>
</com.dengzi.slidingmenu.QQSlidingMenu>
