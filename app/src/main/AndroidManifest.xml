<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dengzi.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".trackcolor.TrackActivity"></activity>
        <activity android:name=".letterside.LetterSideActivity"></activity>
        <activity android:name=".ItemActivity"></activity>
        <activity android:name=".DrawActivity"></activity>
        <activity android:name=".StepActivity"></activity>
    </application>

</manifest>